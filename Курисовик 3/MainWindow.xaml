<Window x:Class="Курисовик_3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Курисовик_3"
        mc:Ignorable="d"
        Title="MainWindow" Height="589" Width="1034" Background="#FF292929">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="90"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>



        </Grid.RowDefinitions>
        <Image Source="Resources/Mask Group.png" Margin="0,0,633,0" HorizontalAlignment="Left"></Image>
        <TextBlock HorizontalAlignment="Center" TextWrapping="Wrap" Text="Меню" VerticalAlignment="Center" Foreground="White" FontSize="48"/>
        <Frame ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden" Grid.Row="1" Name="MainFrame"/>
        <Button x:Name="backButton" Content="Назад" HorizontalAlignment="Left" Margin="834,45,0,0" VerticalAlignment="Top" Width="133" Background="#FF373737" BorderBrush="#FF060606"  Height="32" Foreground="White" FontSize="18" Click="backButton_Click"/>

    </Grid>
</Window>
